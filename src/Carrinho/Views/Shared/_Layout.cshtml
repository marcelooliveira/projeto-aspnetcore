@using Carrinho.Core
@*@using System.Web.Optimization*@

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - by Marcelo Ricardo de Oliveira</title>
    <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/css/site.css" rel="stylesheet" type="text/css" />
    <link href="~/fonts/font-awesome.css" rel="stylesheet" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body class="body">
    <div class="overlay">
        <div class="loader">Loading...</div>
    </div>

    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container" id="container-header">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div class="logo"></div>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        <br />
        <br />
        <br />
        <br />
        @RenderBody()
        <hr />
        <footer>
            <div id="footer-social-links" class="footer-social-links">
                <div class="footer-wrapper">
                    <div>Desenvolvido para</div>
                    <a href="http://casadocodigo.com.br" target="_blank">
                        <h2 class="casadocodigo-footer-logo title"></h2>
                    </a>
                    <ul class="social-list">
                        <li class="social-items">
                            <i class="wm-icon wm-icon-circle icon-facebook"></i><span class="social-name"></span>
                        </li>
                        <li class="social-items">
                            <i class="wm-icon wm-icon-circle icon-twitter"></i><span class="social-name"></span>
                        </li>
                        <li class="social-items">
                            <i class="wm-icon wm-icon-circle icon-google-plus"></i><span class="social-name"></span>
                        </li>
                        <li class="social-items">
                            <i class="wm-icon wm-icon-circle icon-instagram"></i><span class="social-name"></span>
                        </li>
                        <li class="social-items">
                            <i class="wm-icon wm-icon-circle icon-youtube"></i><span class="social-name"></span>
                        </li>
                    </ul>
                </div>
            </div>

            <div id="footer-institutional" class="footer-institutional">
                <div class="footer-wrapper" itemscope="" itemtype="//schema.org/LocalBusiness">
                    <p>
                        <span itemprop="name">desenvolvido por</span>
                        <br /><span itemprop="name">MARCELO RICARDO DE OLIVEIRA</span>
                        <br /><span itemprop="address" itemscope="" itemtype="//schema.org/PostalAddress">
                        </span><a href="mclricardo@gmail.com" itemprop="email">mclricardo@gmail.com</a>
                    </p>
                </div>
            </div>
        </footer>
    </div>
    <div id="loading" class="loading" style="display: none;">Loading&#8230;</div>

    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
</body>
</html>